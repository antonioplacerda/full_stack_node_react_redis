{
  "name": "full_stack_node_react_redis",
  "version": "1.0.0",
  "description": "create a full stack app using react node and redis",
  "main": "app.js",
  "dependencies": {
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "redis": "^2.8.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.2",
    "@types/node": "^13.5.0",
    "@types/node-fetch": "^2.5.4",
    "@types/redis": "^2.8.14",
    "concurrently": "^2.2.0",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "concurrently 'npm run start:worker' 'npm run start:api'",
    "start:worker": "node dist/worker",
    "start:api": "node dist/api",
    "build": "tsc",
    "dev:worker": "concurrently --kill-others \"tsc -w\" \"nodemon server/dist/worker\"",
    "dev:api": "concurrently --kill-others \"tsc -w\" \"nodemon server/dist/api\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonioplacerda/full_stack_node_react_redis.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "react",
    "redis"
  ],
  "author": "Antonio P. Lacerda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antonioplacerda/full_stack_node_react_redis/issues"
  },
  "homepage": "https://github.com/antonioplacerda/full_stack_node_react_redis#readme"
}
